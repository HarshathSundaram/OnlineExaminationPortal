<h1>Students</h1>
<%if @students.blank?%>
    <h2>There is no Students</h3>
<%else%>
    <p>List of Students</p>
    <ul>
        <%@students.each do |student|%>
            <li><%= link_to student.name, student_path(student) %></li>
        <%end%>
    </ul>
<%end%>

<%= link_to "New Student", new_student_path %>