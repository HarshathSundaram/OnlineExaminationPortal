<%= form_with model: @student do |form|%>
    <div>
        <%= form.label :name%>
        <%= form.text_field :name%>
    </div>
    <div>
        <%= form.label :email%>
        <%= form.text_field :email%>
    </div>
    <div>
        <%= form.label :gender%>
        <%= form.select :gender, ['Male','Female']%>
    </div>
    <div>
        <%= form.label :department%>
        <%= form.text_field :department%>
    </div>
    <div>
        <%= form.label :year%>
        <%= form.select :year, ['I', 'II', 'III', 'IV', 'V']%>
    </div>
    <div>
        <%= form.submit%>
    </div>
<%end%>